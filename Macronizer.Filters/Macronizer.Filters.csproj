<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <NeutralLanguage>en-US</NeutralLanguage>
	<Version>0.0.3</Version>
	<FileVersion>0.0.3</FileVersion>
	<AssemblyVersion>0.0.3</AssemblyVersion>
	<Authors>Daniele Fusi</Authors>
	<Company>Fusi</Company>
	<Product>Cursor</Product>
	<Description>Macronizer filters.</Description>
	<Copyright>by Daniele Fusi 2022</Copyright>
	<NeutralLanguage>en-US</NeutralLanguage>
	<PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
	<PackageTags>macronizer</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\Unicode.txt" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Assets\Unicode.txt" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Fusi.Text" Version="3.0.0" />
    <PackageReference Include="Proteus.Core" Version="4.0.0" />
  </ItemGroup>

</Project>
